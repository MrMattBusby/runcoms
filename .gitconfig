[user]
	name    = Matt Busby
	email   = m_busby
[alias]
	st      = status
	co      = checkout
	ci      = commit
	reci    = commit --amend --no-edit
	save    = !git add -A && git commit -m 'chore: commit save point'
	done    = !git push origin HEAD
	br      = branch
	del     = branch -D
	pr      = pull --rebase
	undo    = reset HEAD~1 --mixed
	hist    = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	last    = log -3 HEAD
	type    = cat-file -t
	dump    = cat-file -p
	unstage = reset HEAD --
	restart = ~git reset --hard
	vis     = !gitk

